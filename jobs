    <script>

$(window).on('resize', function() {

		console.log("width"+$(window).width());
    if($(window).width() < 991) {
        $('.navbar').removeClass('standard-height');
	  


    } else{ 
    
		$('.navbar').addClass('standard-height');	  
		$('.navbar').addClass('standard-nav-padding-left');
	    $('.navbar').addClass('standard-nav-padding-right');
		}

   
       if($(window).width()<1353){

            $('.navbar').removeClass('standard-nav-padding-left');
			$('.navbar').removeClass('standard-nav-padding-right');

		} else {
   

		}
});
	</script>

